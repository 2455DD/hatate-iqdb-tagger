<Window x:Class="Hatate.Compare"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hatate"
        mc:Ignorable="d"
        Title="Compare" Height="204.475" Width="236.488">
	<Grid>
		<Image x:Name="Image_Original" HorizontalAlignment="Left" Height="100" Margin="10,10,0,0" VerticalAlignment="Top" Width="100"/>
		<Image x:Name="Image_Match" HorizontalAlignment="Left" Height="100" Margin="115,10,0,0" VerticalAlignment="Top" Width="100"/>
		<Label Content="Original" HorizontalAlignment="Left" Margin="10,110,0,0" VerticalAlignment="Top" Width="100" HorizontalContentAlignment="Center"/>
		<Label Content="Match" HorizontalAlignment="Left" Margin="115,110,0,0" VerticalAlignment="Top" Width="100" HorizontalContentAlignment="Center"/>
		<Button Content="Good?" HorizontalAlignment="Left" Margin="10,141,0,0" VerticalAlignment="Top" Width="205" Click="Button_Good_Click"/>
	</Grid>
</Window>
